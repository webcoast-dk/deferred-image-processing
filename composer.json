{
    "name": "webcoast/deferred-image-processing",
    "type": "typo3-cms-extension",
    "description": "Handles image processing on request instead of during page generation",
    "keywords": ["typo3", "cms", "image", "processing", "scaling", "defer", "deferred"],
    "homepage": "https://github.com/webcoast-dk/deferred-image-processing",
    "license": "GPL-3.0-or-later",
    "extra": {
        "branch-alias": {
            "dev-main": "3.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "deferred_image_processing"
        }
    },
    "require": {
        "php": "^8.1",
        "typo3/cms-core": "^12.4 || ^13.4",
        "typo3/cms-frontend": "^12.4 || ^13.4"
    },
    "autoload": {
        "psr-4": {
            "WEBcoast\\DeferredImageProcessing\\": "Classes/"
        }
    }
}
